{
  "name": "linuxgateproxy",
  "version": "1.0.0",
  "description": "Linux Gate Proxy authentication gateway",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "bash -c 'export NVM_DIR=\"$HOME/.nvm\" && [ -s \"$NVM_DIR/nvm.sh\" ] && . \"$NVM_DIR/nvm.sh\" && nvm use 20 && node src/server.js'",
    "dev": "bash -c 'export NVM_DIR=\"$HOME/.nvm\" && [ -s \"$NVM_DIR/nvm.sh\" ] && . \"$NVM_DIR/nvm.sh\" && nvm use 20 && NODE_ENV=development node --watch src/server.js'",
    "test": "bash -c 'export NVM_DIR=\"$HOME/.nvm\" && [ -s \"$NVM_DIR/nvm.sh\" ] && . \"$NVM_DIR/nvm.sh\" && nvm use 20 && node --test'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@simplewebauthn/server": "^13.2.2",
    "adm-zip": "^0.5.16",
    "better-sqlite3": "^12.4.1",
    "cookie-parser": "^1.4.7",
    "csurf": "^1.11.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "http-proxy": "^1.18.1",
    "kerberos": "^2.0.1",
    "ldapts": "^8.0.9",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10"
  },
  "devDependencies": {
    "supertest": "^7.1.4"
  }
}
